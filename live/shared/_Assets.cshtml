@inherits Custom.Hybrid.Razor12
@using ToSic.Razor.Blade;
@{
  // 1. Get Services for Css and Page
  var pageCss = GetService<Connect.Koi.ICss>();         // Service to get CSS information about the current Theme
  var page = GetService<ToSic.Sxc.Web.IPageService>();  // Service to set titles etc. on the page

  // 2. Include Bootstrap4 if the framework isn't known
  if (pageCss.IsUnknown) {
    page.Activate("Bootstrap4");
  }

  page.Activate("turnOn", "fancybox"); // see https://r.2sxc.org/turnon
  var mobiusDomId = "app-events6-" + @CmsContext.Module.Id;
}
<turnOn turn-on='{"await": ["window.appEvents6"], "run": "window.appEvents6.init()", "data": { "domId": "@mobiusDomId" }}'></turnOn>
<link rel="stylesheet" href="@App.Path/live/dist/styles.min.css" data-enableoptimizations="true">
<script src="@App.Path/live/dist/scripts.min.js" data-enableoptimizations="false"></script>
