@inherits Custom.Hybrid.Razor14
@using ToSic.Razor.Blade
@{
  // 2. Include Bootstrap4 if the framework isn't known
  if (Kit.Css.IsUnknown) {
    Kit.Page.Activate("Bootstrap4");
  }

  Kit.Page.Activate("turnOn"); // see https://r.2sxc.org/turnon

  var domAttribute = "events-" + CmsContext.Module.Id;

  var Edition = CmsContext.View.Edition;
}

@Edit.Enable(js: true)
<turnOn turn-on='{"run": "window.appEvents6.init()", "data": { "domAttribute": "@domAttribute" }}'></turnOn>
<link rel="stylesheet" href="@App.Path/@Edition/dist/styles.min.css" @Kit.Page.AssetAttributes()>
<script src="@App.Path/@Edition/dist/scripts.min.js" @Kit.Page.AssetAttributes()></script>
